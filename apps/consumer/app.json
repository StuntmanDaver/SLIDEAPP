{
  "expo": {
    "name": "Slide",
    "slug": "slide-consumer",
    "version": "0.1.0",
    "scheme": "slide",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E1E2DD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-apple-authentication",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your face ID?"
        }
      ]
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.slideapp.consumer",
      "buildNumber": "1",
      "usesAppleSignIn": true,
      "associatedDomains": [
        "applinks:slidenightlife.com",
        "appclips:slidenightlife.com"
      ],
      "infoPlist": {
        "NSFaceIDUsageDescription": "Use Face ID to securely authenticate"
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.com.slideapp.consumer"
        ]
      }
    },
    "android": {
      "package": "com.slidevenue.consumer",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "slidenightlife.com",
              "pathPrefix": "/claim"
            },
            {
              "scheme": "slide",
              "host": "claim"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2b6f7543-7d84-44fc-a9c4-3873292ef98d"
      }
    },
    "owner": "stuntmandaver"
  }
}
